@model IEnumerable<WebApp.Models.Plan_Fact>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ActualNumberOfShipmentsByDay.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PlannedNumberOfShipments)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActualNumberOfShipments)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ActualNumberOfShipmentsByDay.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PlannedNumberOfShipments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActualNumberOfShipments)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
